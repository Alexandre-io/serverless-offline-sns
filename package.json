{
  "name": "serverless-offline-sns",
  "version": "0.68.0",
  "description": "Serverless plugin to run a local SNS server and call lambdas with events notifications.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p src",
    "watch": "tsc -p src -w",
    "test": "nyc ts-mocha --slow 300 -p src/tsconfig.json test/**/*.ts",
    "lint": "tslint -p src/",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mj1618/serverless-offline-sns.git"
  },
  "keywords": [
    "serverless-plugin",
    "serverless",
    "sns",
    "offline",
    "localstack"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Matthew James <matthew.stephen.james@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mj1618/serverless-offline-sns/issues"
  },
  "homepage": "https://github.com/mj1618/serverless-offline-sns#readme",
  "dependencies": {
    "aws-sdk": "^2.761.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.0",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.4",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  }
}
